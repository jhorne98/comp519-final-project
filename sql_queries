S0
SELECT COUNT(*) FROM onek_integer i WHERE i.parent IS NULL AND NOT EXISTS (SELECT 1 FROM onek_integer t WHERE i.id = t.parent)

S4
SET SESSION max_recursive_iterations = 4;
WITH RECURSIVE cte_parent_child AS (
    SELECT id FROM onek_integer WHERE parent IS NULL
    UNION ALL
    SELECT t.id FROM onek_integer t INNER JOIN cte_parent_child cte ON t.parent = cte.id
)
SELECT COUNT(*) from cte_parent_child;

S128
SET SESSION max_recursive_iterations = 128;

S256
SET SESSION max_recursive_iterations = 256;

S512
SET SESSION max_recursive_iterations = 512;

I1
SELECT COUNT(*) FROM onek_integer WHERE payload = 65535;

I2
SELECT COUNT(*) FROM onek_integer WHERE payload < 32767;

C1
SELECT COUNT(*) FROM onek_char8k WHERE payload LIKE '%asdf%';